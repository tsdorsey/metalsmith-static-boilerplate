{
  "name": "metalsmith-static-boilerplate",
  "scripts": {
    "start": "npm run servedev",
    "builddev": "NODE_ENV=development npm run build",
    "buildprod": "NODE_ENV=production npm run build",
    "servedev": "npm run reset && NODE_ENV=development npm run serve",
    "serveprod": "npm run reset && NODE_ENV=production npm run serve",
    "build": "node build.js",
    "serve": "npm run browser_sync & nodemon --exec 'npm run build && browser-sync reload'",
    "browser_sync": "browser-sync --no-notify start --server build --browser=chrome",
    "reset": "ps aux | grep -E '([b]rowser-sync)|([n]odemon)' | awk '{print $2}' | while read line ; do kill -9 $line ; done"
  },
  "author": "Trevor Dorsey <tsdorsey@gmail.com> (http://foyyay.com)",
  "repository": {
    "type": "git",
    "url": "git@github.com:tsdorsey/metalsmith-static-boilerplate.git"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "bourbon": "4.2.7",
    "browserify": "13.1.1",
    "handlebars": "4.0.5",
    "metalsmith": "2.2.2",
    "metalsmith-autoprefixer": "1.1.1",
    "metalsmith-browserify-alt": "1.0.1",
    "metalsmith-clean-css": "5.0.0",
    "metalsmith-html-minifier": "2.2.0",
    "metalsmith-ignore": "0.1.2",
    "metalsmith-in-place": "1.4.4",
    "metalsmith-inline-svg": "0.1.3",
    "metalsmith-layouts": "1.6.5",
    "metalsmith-move-up": "1.0.0",
    "metalsmith-rewrite": "0.1.2",
    "metalsmith-robots": "1.0.1",
    "metalsmith-sass": "1.3.0",
    "metalsmith-sitemap": "1.1.0",
    "metalsmith-uglifyjs": "0.2.1"
  },
  "devDependencies": {
    "browser-sync": "2.17.5",
    "nodemon": "1.11.0"
  }
}
